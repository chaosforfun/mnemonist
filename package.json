{
  "name": "mnemonist",
  "version": "0.12.0",
  "description": "Curated collection of data structures for the JavaScript language.",
  "main": "endpoint.js",
  "scripts": {
    "lint": "eslint ./*.js ./utils ./test",
    "prepublish": "npm run lint && npm test",
    "test": "mocha"
  },
  "files": [
    "utils",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yomguithereal/mnemonist.git"
  },
  "keywords": [
    "data structures",
    "structures",
    "heap",
    "fibonacci heap",
    "stack",
    "queue",
    "inverted index",
    "linked list",
    "trie",
    "bag",
    "multiset",
    "multimap",
    "counter",
    "suffix tree",
    "symspell",
    "bk tree",
    "burkhard-keller tree",
    "vp tree",
    "vantage point tree"
  ],
  "author": {
    "name": "Guillaume Plique",
    "url": "http://github.com/Yomguithereal"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yomguithereal/mnemonist/issues"
  },
  "homepage": "https://github.com/yomguithereal/mnemonist#readme",
  "devDependencies": {
    "@yomguithereal/eslint-config": "^3.0.0",
    "damerau-levenshtein": "^1.0.3",
    "eslint": "^3.8.1",
    "leven": "^2.0.0",
    "lodash": "^4.17.4",
    "mocha": "^3.1.2"
  },
  "eslintConfig": {
    "extends": "@yomguithereal/eslint-config",
    "globals": {
      "Set": true,
      "Map": true,
      "Symbol": true,
      "Uint8Array": true,
      "Uint16Array": true,
      "Float64Array": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "ecmaFeatures": {
      "forOf": true
    }
  }
}
